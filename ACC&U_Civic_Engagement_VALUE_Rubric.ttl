    asn:qualityLabel "Milestone"@en-US ;
    asn:score "2"^^xsd:integer ;
    asn:benchmark "Has awareness that own attitudes and beliefs are different from those of other cultures and communities. Exhibits little curiosity about what can be learned from diversity of communities and cultures."@en-US ;
    asn:sequence "3"^^xsd:integer ;
    asn:levelFor <https://imaginaryPlace.org/resources/47af1f8a-ca52-4976-beaa-3dd375ec9125> .

